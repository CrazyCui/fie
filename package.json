{
  "private": true,
  "scripts": {
    "test": "istanbul cover _mocha -- --opts test/mocha.opts --timeout 20000 --bail 'packages/**/test/*-spec.js'",
    "test:one": "DEBUG=fie-* istanbul cover _mocha -- --opts test/mocha.opts --bail --timeout 20000",
    "ci-test": "istanbul cover _mocha -- -R mocha-bamboo-reporter --opts test/mocha.opts --bail --timeout 20000 'packages/**/test/*-spec.js' && codecov",
    "ci-log:coverage": "ci-log -r coverage -p ./coverage/lcov.info",
    "ci-log:mocha": "ci-log -r mocha -p ./mocha.json",
    "ci-eslint": "eslint -o eslint.json -f json ./",
    "ci-log:eslint": "ci-log -r eslint -p ./eslint.json",
    "lint": "./node_modules/eslint/bin/eslint.js --fix ./",
    "doc": "jsdoc -c conf.json  ./node_modules/ink-docstrap/template -R README.md -r",
    "contributors": "contributors",
    "postinstall": "lerna bootstrap"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "co-mocha": "^1.1.3",
    "codecov": "^1.0.1",
    "contributors": "^0.5.1",
    "eslint": "^3.8.1",
    "eslint-config-ais": "0.0.16",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^6.3.0",
    "fs-extra": "^1.0.0",
    "ink-docstrap": "^1.3.0",
    "istanbul": "^0.4.4",
    "jsdoc": "^3.4.2",
    "lerna": "2.0.0-beta.30",
    "mm": "^2.0.0",
    "mocha": "^3.1.2",
    "mocha-bamboo-reporter": "^1.1.1",
    "mocha-generators": "^1.2.0",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "supertest": "^2.0.1"
  },
  "engine": {
    "node": ">4.2.1"
  }
}
